@model IEnumerable<Medicure_Entity.Drug>

@{
    ViewData["Title"] = "GetallDrugs";
}

<h1>GetallDrugs</h1>


<table class="table h4">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Required_Qty)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Instock_Qty)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Price)

            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Required_Qty)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Instock_Qty)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price)
            </td>

        </tr>
}
    </tbody>
</table>
<div>
    <a class="btn btn-outline-light"  asp-controller="Patient" asp-action="Index" asp-route-id="@ViewBag.Id">Back</a>
</div>

